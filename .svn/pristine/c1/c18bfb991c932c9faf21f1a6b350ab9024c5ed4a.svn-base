<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/margin_10"
        android:background="@color/ac_bg" />

    <TextView
        android:id="@+id/lad_dele"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/margin_8"
        android:layout_marginRight="@dimen/margin_8"
        android:drawableLeft="@mipmap/ic_tudi"
        android:drawablePadding="@dimen/margin_8"
        android:drawableRight="@mipmap/ic_shanchu_b"
        android:gravity="center_vertical"
        android:paddingBottom="@dimen/margin_10"
        android:paddingTop="@dimen/margin_10"
        android:text="土地"
        android:textColor="@color/black"
        android:textSize="@dimen/text_15" />


    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/margin_8"
        android:background="@color/ac_bg" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/margin_10"
            android:layout_marginLeft="@dimen/margin_8"
            android:layout_marginTop="@dimen/margin_10"
            android:layout_weight="1"
            android:drawableLeft="@mipmap/zhongdian"
            android:drawablePadding="@dimen/margin_5"
            android:text="面积需求"
            android:textColor="@color/black"
            android:textSize="@dimen/text_15" />

        <EditText
            android:id="@+id/lad_mj"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="@dimen/margin_5"
            android:background="@null"
            android:hint="填入面积需求数量"
            android:inputType="numberDecimal"
            android:maxLines="1"
            android:singleLine="true"
            android:textAlignment="textEnd"
            android:textColorHint="@color/black_40"
            android:textColor="@color/black_80"
            android:textSize="@dimen/text_14"
            tools:ignore="RtlCompat" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="@dimen/margin_8"
            android:text="亩"
            android:textColor="@color/black"
            android:textSize="@dimen/text_14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/margin_8"
        android:background="@color/ac_bg" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/margin_10"
            android:layout_marginLeft="@dimen/margin_8"
            android:layout_marginTop="@dimen/margin_10"
            android:layout_weight="1"
            android:drawableLeft="@mipmap/zhongdian"
            android:drawablePadding="@dimen/margin_5"
            android:text="区位要求"
            android:textColor="@color/black"
            android:textSize="@dimen/text_15" />

        <TextView
            android:id="@+id/lad_qw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="@dimen/margin_8"
            android:drawablePadding="@dimen/margin_5"
            android:drawableRight="@mipmap/go"
            android:hint="选择区位"
            android:textAlignment="textEnd"
            android:textColorHint="@color/black_40"
            android:textColor="@color/black_80"
            android:textSize="@dimen/text_14" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginLeft="@dimen/margin_8"
        android:background="@color/ac_bg" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/margin_10"
            android:layout_marginLeft="@dimen/margin_8"
            android:layout_marginTop="@dimen/margin_10"
            android:layout_weight="1"
            android:drawableLeft="@mipmap/zhongdian"
            android:drawablePadding="@dimen/margin_5"
            android:text="土地性质"
            android:textColor="@color/black"
            android:textSize="@dimen/text_15" />

        <TextView
            android:id="@+id/lad_xz"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_marginRight="@dimen/margin_8"
            android:drawablePadding="@dimen/margin_5"
            android:drawableRight="@mipmap/go"
            android:hint="选择土地"
            android:textAlignment="textEnd"
            android:textColorHint="@color/black_40"
            android:textColor="@color/black_80"
            android:textSize="@dimen/text_14" />
    </LinearLayout>


</LinearLayout>