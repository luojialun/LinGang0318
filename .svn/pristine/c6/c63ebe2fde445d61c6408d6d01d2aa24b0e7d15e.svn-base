        private String oaNum;
        private String recentlyAddedNum;
        private String reviewNum;
        private String systemNum;

        public String getReviewNum() {
            return reviewNum;
        }

        public String getEnterpriseNoticeNum() {
            return enterpriseNoticeNum;
        }

        public String getOaNum() {
            return oaNum;
        }

        public String getRecentlyAddedNum() {
            return recentlyAddedNum;
        }

        public String getSystemNum() {
            return systemNum;
        }
    }

}
